    "email": "dennykorsukewitz@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dennykorsukewitz/VSCode-QuoteWithMarker"
  },
  "sponsor": {
    "url": "https://github.com/sponsors/dennykorsukewitz"
