  "publisher": "dennykorsukewitz",
  "icon": "doc/images/icon.png",
  "license": "SEE LICENSE IN LICENSE",
  "homepage": "https://github.com/dennykorsukewitz/VSCode-QuoteWithMarker/blob/dev/README.md",
  "pricing": "Free",
  "bugs": {
